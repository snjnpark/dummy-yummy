{
  "manifest_version": 3,
  "name": "dummy-yummy",
  "version": "1.0",
  "description": "귀찮은 웹 양식을 더미 데이터로 채워드립니다.",
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
